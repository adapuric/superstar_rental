<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-10 col-sm-offset-1">
      <h1>Create a superstar profile</h1>
      <%= simple_form_for @superstar do |f| %>
        <%= f.input :name %>
        <%= f.input :event_type %>
        <%= f.input :bio %>
        <%= f.input :price %>
        <%= f.input :region %>
        <%= f.input :image_url %>
        <%= f.submit class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
